{
  "type": "delphi deploy config",
  "version": 1,
  "actions": [
    
    "// compile",
    {
      "type": "compile-coffee",
      "src": "site/fs_data.coffee",
    },
    {
      "type": "compile-coffee",
      "src": "site/fs_plot.coffee",
    },
    
    "// minimize",
    {
      "type": "minimize-js",
      "src": "site/fs_data.js",
    },
    {
      "type": "minimize-js",
      "src": "site/fs_plot.js",
    },
    
    "// make changelog",
    {
      "type": "move",
      "src": "README.md",
      "dst": "site/changelog.txt"
    },
    
    "// web sources",
    {
      "type": "move",
      "src": "site/",
      "dst": "/var/www/html/misc/fluscores/",
      "match": "^.*\\.(html|js|txt)$",
      "add-header-comment": true
    }
    
  ]
}
